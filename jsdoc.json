{
    "tags": {
        "allowUnknownTags": false
    },
    "source": {
        "include": [
            "web-app/browser/scripts/board.js", "web-app/browser/scripts/constants.js", "web-app/browser/scripts/controller.js", "web-app/browser/scripts/main.js", "web-app/browser/scripts/view.js"

        ],
        "includePattern": "\\.js$",
        "excludePattern": "(node_modules/|docs)"
    },
    "plugins": [
        "plugins/markdown"
    ],
    "opts": {
        "template": "node_modules/docdash",
        "encoding": "utf8",
        "destination": "docs/",
        "recurse": true,
        "verbose": true
    },
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": false
    },
    "docdash": {
        "collapse": false
    }
}
